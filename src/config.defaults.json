{
    "chunkCacheSize": 32,
    "paths": ["/greyhound", "~/greyhound", "/entwine", "~/entwine", "/opt/data"],
    "resourceTimeoutMinutes": 30,
    "http": {
        "port": 8080,
        "headers": {
            "Cache-Control":                  "public, max-age=300",
            "Access-Control-Allow-Origin":    "*",
            "Access-Control-Allow-Methods":   "GET,PUT,POST,DELETE"
        }
    }
}

